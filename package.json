{
  "name": "vfmtoolkit",
  "displayName": "vfmtoolkit",
  "description": "As the name suggests, a vfm debug toolkit",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vfmtoolkit.fileExplorer",
    "onView:vfmtoolkit.customUIView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vfmtoolkit",
          "title": "VFMToolkit",
          "icon": "$(tools)"
        }
      ]
    },
    "views": {
      "vfmtoolkit": [
        {
          "id": "vfmtoolkit.fileExplorer",
          "name": "File Explorer",
          "icon": "resources/icon.svg"
        },
        {
          "id": "vfmtoolkit.customUIView",
          "name": "连接设备",
          "icon": "$(tools)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
